      <label><%= f.radio_button :annual, :"11-20年目" %>11-20年目</label>
      <label><%= f.radio_button :annual, :"21-30年目" %>21-30年目</label>
      <label><%= f.radio_button :annual, :"31年目以上" %>31年目以上</label>
    </div>

    <div class="field">
