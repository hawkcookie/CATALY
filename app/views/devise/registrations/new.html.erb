<div class="new_registration">

  <div class="button">

    <div class="research_title">
      <h2>新規登録</h2>
    </div>


    <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
    <%= render "devise/shared/error_messages", resource: resource %>

    <div class="field">
      <div class="my_profile_content">
        <%= f.label :email %> 　<div class="mandatory">必須</div><br />
      </div>
      <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
    </div>


    <div class="field">
      <div class="my_profile_content">
        <%= f.label :password %> 　<div class="mandatory">必須</div>
      </div>
      <% if @minimum_password_length %>
      <em>(<%= @minimum_password_length %> 文字以上で設定してください。)</em>
      <% end %><br />
      <%= f.password_field :password, autocomplete: "new-password" %>
    </div>

    <div class="field">
      <div class="my_profile_content">
        <%= f.label :password_confirmation %> 　<div class="mandatory">必須</div><br />
      </div>
      <%= f.password_field :password_confirmation, autocomplete: "new-password" %>
    </div>

    <!--画像登録-->
    <div class="field">
      <div class="my_profile_content">
        <%= f.label :トップ画像 %> 　<div class="mandatory">必須</div><br />
      </div>
      <%= f.file_field :image %>
    </div>


    <div class="field">
      <div class="my_profile_content">
        <%= f.label :name %> 　<div class="mandatory">必須</div> <br />
      </div>
      <%= f.text_field :name %>
    </div>

    <!--出身大学-->
    <div class="field">
      <div class="my_profile_content">
        <%= f.label :出身大学 %> <br />
      </div>
      <%= f.text_field :uni %>
    </div>


    　
    <!--チェックボックス-->

    <div class="field">
      <div class="my_profile_content">
        <%= f.label :sex %> 　<div class="mandatory">必須</div><br />
      </div>
      <div class="label_css">
        <label><%= f.radio_button :sex, :"男性" %>男性</label>
        <label><%= f.radio_button :sex, :"女性" %>女性</label>
        <label><%= f.radio_button :sex, :"その他" %>その他</label>
      </div>
    </div>


    <div class="field">
